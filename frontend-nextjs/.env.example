# SoldierIQ Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Backend API URL (Public - exposed to browser)
NEXT_PUBLIC_API_URL=http://localhost:8001

# iDriveE2 S3-Compatible Storage Configuration (Server-side only)
IDRIVEE2_REGION=us-east-1
IDRIVEE2_ENDPOINT_URL=https://s3.us-west-1.idrivee2.com
IDRIVEE2_ACCESS_KEY_ID=your_access_key_here
IDRIVEE2_SECRET_ACCESS_KEY=your_secret_key_here
IDRIVEE2_BUCKET_NAME=soldieriq-documents
